CREATE TABLE `obs_employeecontactinfo` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `email` varchar(45) DEFAULT NULL,
  `personal_mobileNo` varchar(45) DEFAULT NULL,
  `alternate_mobileNo` varchar(45) DEFAULT NULL,
  `current_Address_Line1` varchar(45) DEFAULT NULL,
  `current_Address_Line2` varchar(45) DEFAULT NULL,
  `current_City` varchar(45) DEFAULT NULL,
  `current_State` int(15) DEFAULT NULL,
  `current_Pin` int(15) DEFAULT NULL,
  `permanent_Address_Line_1` varchar(45) DEFAULT NULL,
  `emp_Id` varchar(45) DEFAULT NULL,
  `created_By` varchar(45) DEFAULT NULL,
  `updated_By` varchar(45) DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `updated_date` datetime DEFAULT NULL,
  `flag` tinyint(1) DEFAULT NULL,
  PRIMARY KEY (`id`)
) 